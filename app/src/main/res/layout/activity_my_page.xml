<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context=".mypage.MyPageActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:minHeight="?attr/actionBarSize"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:titleTextColor="@color/repeat_gray" />

    <View
        android:id="@+id/view2"
        android:layout_width="0dp"
        android:layout_height="16dp"
        android:background="#f5f5f5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <TextView
        android:id="@+id/view_nickname"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:background="@drawable/bottom_border"
        android:text="@string/label_nickname"
        android:textSize="16sp"
        android:gravity="center_vertical"
        android:paddingStart="16dp"
        android:paddingTop="11dp"
        android:paddingBottom="13dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <View
        android:id="@+id/view3"
        android:layout_width="0dp"
        android:layout_height="16dp"
        android:background="#f5f5f5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_nickname" />

    <TextView
        android:id="@+id/view_tutorial"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:background="@drawable/bottom_border"
        android:gravity="center_vertical"
        android:paddingStart="16dp"
        android:paddingTop="11dp"
        android:paddingBottom="13dp"
        android:text="@string/label_tutorial"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view3" />

    <TextView
        android:id="@+id/view_opinion"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:background="@drawable/bottom_border"
        android:gravity="center_vertical"
        android:paddingStart="16dp"
        android:paddingTop="11dp"
        android:paddingBottom="13dp"
        android:text="@string/label_opinion"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_tutorial" />

    <TextView
        android:id="@+id/view_faq"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:background="@drawable/bottom_border"
        android:gravity="center_vertical"
        android:paddingStart="16dp"
        android:paddingTop="11dp"
        android:paddingBottom="13dp"
        android:text="@string/label_faq"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_opinion" />

    <View
        android:id="@+id/view4"
        android:layout_width="0dp"
        android:layout_height="16dp"
        android:background="#f5f5f5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_faq" />

    <TextView
        android:id="@+id/view_terms"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:background="@drawable/bottom_border"
        android:gravity="center_vertical"
        android:paddingStart="16dp"
        android:paddingTop="11dp"
        android:paddingBottom="13dp"
        android:text="@string/label_terms"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view4" />

    <TextView
        android:id="@+id/view_policy"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:background="@drawable/bottom_border"
        android:gravity="center_vertical"
        android:paddingStart="16dp"
        android:paddingTop="11dp"
        android:paddingBottom="13dp"
        android:text="@string/label_policy"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_terms" />

    <TextView
        android:id="@+id/view_version"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:background="@drawable/bottom_border"
        android:gravity="center_vertical"
        android:paddingStart="16dp"
        android:paddingTop="11dp"
        android:paddingBottom="13dp"
        android:text="@string/label_version"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_policy" />

    <View
        android:id="@+id/view5"
        android:layout_width="0dp"
        android:layout_height="16dp"
        android:background="#f5f5f5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_version" />

    <TextView
        android:id="@+id/view_init"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:background="@drawable/bottom_border"
        android:gravity="center_vertical"
        android:paddingStart="16dp"
        android:paddingTop="11dp"
        android:paddingBottom="13dp"
        android:text="@string/label_init"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view5" />

    <TextView
        android:id="@+id/view_logout"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:background="@drawable/bottom_border"
        android:gravity="center_vertical"
        android:paddingStart="16dp"
        android:paddingTop="11dp"
        android:paddingBottom="13dp"
        android:text="@string/label_logout"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_init" />

    <TextView
        android:id="@+id/view_unregister"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:background="@drawable/bottom_border"
        android:gravity="center_vertical"
        android:paddingStart="16dp"
        android:paddingTop="11dp"
        android:paddingBottom="13dp"
        android:text="@string/label_unregister"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_logout" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toTopOf="@+id/view3"
        app:layout_constraintEnd_toEndOf="@+id/view_nickname"
        app:layout_constraintTop_toBottomOf="@+id/view2"
        app:srcCompat="@drawable/ic_nav_mark_666" />

    <TextView
        android:id="@+id/tv_nickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="@+id/imageView6" />

    <TextView
        android:id="@+id/tv_latest_versioncode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:textSize="16sp"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/view5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_policy" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="/"
        android:textSize="16sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tv_latest_versioncode"
        app:layout_constraintEnd_toStartOf="@+id/tv_latest_versioncode" />

    <TextView
        android:id="@+id/tv_current_versioncode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text=""
        android:textSize="16sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/tv_latest_versioncode"
        app:layout_constraintEnd_toStartOf="@+id/textView22" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/loading_layout_mypage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/mypage_progressbar"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:lottie_fileName="loading.json" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/tutorial"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/toolbar"
        app:srcCompat="@drawable/tutorial1" />

</androidx.constraintlayout.widget.ConstraintLayout>